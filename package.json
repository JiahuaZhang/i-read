    "@remix-run/serve": "^2.11.2",
    "@supabase/supabase-js": "^2.45.1",
    "antd": "^5.20.1",
    "epub": "^1.2.1",
    "google-auth-library": "^9.13.0",
    "googleapis": "^140.0.1",
    "isbot": "^4.1.0",
    "jotai": "^2.9.3",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.17.0",
    "lodash": "^4.17.21",
    "node-html-parser": "^6.1.13",
    "rangy": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.6.0",
    "tiny-invariant": "^1.3.3"
